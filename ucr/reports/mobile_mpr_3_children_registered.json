{
  "domains": [
    "icds-cas",
    "icds-dashboard-qa",
    "reach-test"
  ],
  "server_environment": [
    "softlayer",
    "icds-new"
  ],
  "report_id": "static-mobile_mpr_3_children_registered",
  "data_source_table": "static-person_cases_v2",
  "config": {
    "title": "MPR 3 - Children Registered (Static)",
    "description": "New children registered during month.  Displays person cases grouped by month opened, owner ID, and age group at time of registration",
    "visible": false,
    "aggregation_columns": [
      "owner_id",
      "month",
      "age_group"
    ],
    "filters": [
      {
        "display": "Date Case Opened",
        "slug": "opened_on",
        "type": "date",
        "field": "opened_on",
        "datatype": "date"
      },
      {
        "slug": "age_at_registration_in_bounds",
        "type": "pre",
        "field": "age_at_reg",
        "pre_operator": "between",
        "pre_value": [0, 6],
        "datatype": "integer"
      },
      {
        "display": "Filter by AWW",
        "slug": "awc_id",
        "type": "dynamic_choice_list",
        "field": "awc_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by Supervisor",
        "slug": "supervisor_id",
        "type": "dynamic_choice_list",
        "field": "supervisor_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by Block",
        "slug": "block_id",
        "type": "dynamic_choice_list",
        "field": "block_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by District",
        "slug": "district_id",
        "type": "dynamic_choice_list",
        "field": "district_id",
        "choice_provider": {
          "type": "location"
        }
      },
      {
        "display": "Filter by State",
        "slug": "state_id",
        "type": "dynamic_choice_list",
        "field": "state_id",
        "choice_provider": {
          "type": "location"
        }
      }
    ],
    "columns": [
      {
        "display": {
          "en": "Owner",
          "hin": "Owner"
        },
        "column_id": "owner_id",
        "type": "field",
        "field": "owner_id",
        "aggregation": "simple",
        "transform": {
          "type": "custom",
          "custom_type": "owner_display"
        }
      },
      {
        "display": "Month",
        "column_id": "month",
        "type": "aggregate_date",
        "field": "opened_on",
        "format": "%Y-%m"
      },
      {
        "display": "Age Group",
        "column_id": "age_group",
        "type": "conditional_aggregation",
        "whens": {
            "0 <= age_at_reg AND age_at_reg < 3": "under_3",
            "3 <= age_at_reg AND age_at_reg <= 6": "over_3"
        }
      },
      {
        "display": "F_resident_count",
        "column_id": "F_resident_count",
        "type": "field",
        "field": "F_resident_count",
        "aggregation": "sum",
        "calculate_total": true
      },
      {
        "display": "M_resident_count",
        "column_id": "M_resident_count",
        "type": "field",
        "field": "M_resident_count",
        "aggregation": "sum",
        "calculate_total": true
      },
      {
        "display": "F_migrant_count",
        "column_id": "F_migrant_count",
        "type": "field",
        "field": "F_migrant_count",
        "aggregation": "sum",
        "calculate_total": true
      },
      {
        "display": "M_migrant_count",
        "column_id": "M_migrant_count",
        "type": "field",
        "field": "M_migrant_count",
        "aggregation": "sum",
        "calculate_total": true
      }
    ],
    "sort_expression": [],
    "configured_charts": []
  }
}
